{
  "app": "cv-builder",
  "storeType": "redux",
  "accessPath": "window.__REDUX_DEVTOOLS_EXTENSION__?.instanceConnection?.getState()",
  "alternativeAccessPaths": [
    "window.__REDUX_DEVTOOLS_EXTENSION__?.instanceConnection",
    "window.__REDUX_DEVTOOLS_EXTENSION_COMPOSE__"
  ],
  "version": "2.0.0",
  "updated": "2025-11-16T14:00:00Z",
  "notes": "Access Redux store via DevTools extension for security. Requires Redux DevTools browser extension installed.",
  "queries": {
    "currentTab": {
      "path": "navigation.currentTab",
      "type": "string",
      "values": ["bio", "jobs", "outputs", "interactive"],
      "description": "Currently active dashboard tab key (TabKey enum)"
    },
    "previousTab": {
      "path": "navigation.previousTab",
      "type": "string",
      "values": ["bio", "jobs", "outputs", "interactive"],
      "description": "Previously active dashboard tab key"
    },
    "currentTabIndex": {
      "path": "navigation.currentTabIndex",
      "type": "number",
      "values": [0, 1, 2, 3],
      "description": "Currently active dashboard tab index (0=Bio, 1=Jobs, 2=Outputs, 3=Interactive)"
    },
    "previousTabIndex": {
      "path": "navigation.previousTabIndex",
      "type": "number",
      "values": [0, 1, 2, 3],
      "description": "Previously active dashboard tab index"
    },
    "bioViewMode": {
      "path": "navigation.bioViewMode",
      "type": "string",
      "values": ["landing", "tiles", "files"],
      "description": "Bio panel view mode (landing, tiles, or files view)"
    },
    "chatMessages": {
      "path": "chat.messages",
      "type": "array",
      "description": "Array of chat messages with role (user/assistant) and content"
    },
    "chatMessageCount": {
      "path": "chat.messages.length",
      "type": "number",
      "description": "Number of chat messages"
    },
    "draftInput": {
      "path": "chat.draftInput",
      "type": "string",
      "description": "Current chat input text (draft message being composed)"
    },
    "chatExpanded": {
      "path": "chat.isExpanded",
      "type": "boolean",
      "description": "Whether chat window is expanded to full-screen"
    },
    "chatSummary": {
      "path": "chat.chatSummary",
      "type": "string",
      "description": "Generated summary of chat conversation (2-3 words)"
    },
    "chatLoading": {
      "path": "chat.isLoading",
      "type": "boolean",
      "description": "Whether chat is processing a message"
    },
    "streamingContent": {
      "path": "chat.streamingContent",
      "type": "string",
      "description": "Content currently being streamed from assistant"
    },
    "unreadCount": {
      "path": "chat.unreadCount",
      "type": "number",
      "description": "Number of unread chat messages"
    },
    "lastReadMessageIndex": {
      "path": "chat.lastReadMessageIndex",
      "type": "number",
      "description": "Index of last message user read"
    },
    "scrollPosition": {
      "path": "chat.scrollPosition",
      "type": "number",
      "description": "Saved scroll position in chat"
    },
    "orchestratorInitialized": {
      "path": "agent.isInitialized",
      "type": "boolean",
      "description": "Whether the BrowserOrchestrator agent is initialized"
    },
    "agentError": {
      "path": "agent.error",
      "type": "string",
      "description": "Agent initialization error message (if any)"
    }
  }
}
